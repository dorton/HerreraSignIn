<%- model_class = StudentCheckOut.new.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table id="checkout" class="table table-striped">
  <thead>
    <tr>
      <!-- <th><%= model_class.human_attribute_name(:id) %></th> -->
      <th><%= model_class.human_attribute_name("Date and Time") %></th>
      <th><%= model_class.human_attribute_name(:parent) %></th>
      <th><%= model_class.human_attribute_name(:student) %></th>
      <th><%= model_class.human_attribute_name(:grade) %></th>
      <th><%= model_class.human_attribute_name(:teacher) %></th>
      
      <!-- <th><%=t '.actions', :default => t("helpers.actions") %></th> -->
    </tr>
  </thead>
  <tbody>
    <% @student_check_outs.each do |student_check_out| %>
      <tr>
        <!-- <td><%= link_to student_check_out.id, student_check_out_path(student_check_out) %></td> -->
        <td><%=l student_check_out.created_at.localtime, :format => :long %></td>
        <td><%= student_check_out.parent %></td>
        <td><%= student_check_out.student %></td>
        <td><%= student_check_out.grade %></td>
        <td><%= student_check_out.teacher %></td>
        
        <!-- <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_student_check_out_path(student_check_out), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      student_check_out_path(student_check_out),
                      :method => :delete,
                      :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
                      :class => 'btn btn-mini btn-danger' %>
        </td> -->
      </tr>
    <% end %>
  </tbody>
</table>
