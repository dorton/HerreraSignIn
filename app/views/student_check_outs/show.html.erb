<%- model_class = @student_check_out.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:parent) %>:</strong></dt>
  <dd><%= @student_check_out.parent %></dd>
  <dt><strong><%= model_class.human_attribute_name(:student) %>:</strong></dt>
  <dd><%= @student_check_out.student %></dd>
  <dt><strong><%= model_class.human_attribute_name(:grade) %>:</strong></dt>
  <dd><%= @student_check_out.grade %></dd>
  <dt><strong><%= model_class.human_attribute_name(:teacher) %>:</strong></dt>
  <dd><%= @student_check_out.teacher %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              "/", :class => 'btn btn-large'  %>
